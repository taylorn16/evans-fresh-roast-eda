{
    "private": true,
    "scripts": {
        "start": "npm run setup && dotnet fable watch . --outDir ./fable --sourceMaps --run vite",
        "build": "npm run setup && dotnet fable . --outDir ./fable --sourceMaps --run vite build",
        "serve": "vite serve",
        "setup": "mkdir -p fable &&  cp src/index.html fable/index.html"
    },
    "devDependencies": {
        "@microsoft/signalr": "^6.0.2",
        "remotedev": "^0.2.9",
        "sass": "^1.49.7",
        "vite": "^2.7.13"
    },
    "name": "evans-fresh-roast-client",
    "version": "1.0.0",
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
